  <script type="text/javascript">
    var map;
    // Always load the machine availability since we removed auth
    document.getElementById("loading").style.display = "block";
    document.getElementById("main").style.display = "none";
    var jqxhr = $.ajax({
      url: 'https://script.google.com/macros/s/AKfycby0NxjFPg6nvA9W_l0T4JttQmAEOoGWaj6Cq95yy7Z7dBNoYHukteZFXg/exec',
      method: "GET",
      dataType: "json"
    }).then(function(doRef) {
      var data = doRef["data"];
      console.log(data);
      var list = data.split(",");
      map = {
        "GCCLaser": "No", // Hard coded to No
        // "GCCLaser": list[0],
        "MehtaLaser": list[1],  
        "Vinyl": "No",
        // "Vinyl": list[2],
        "3DPrinter": "No",
        // "3DPrinter": list[3],
        // "UM3+": list[4],
        // "MakerGear": list[5]
      };

      Object.keys(map).forEach(function(key) {
        document.getElementById(key).innerHTML = "Available: " + map[key];

        if (map[key] === "Yes") {
          document.getElementById(key + "-btn").style.display = "block";
          document.getElementById(key + "-calendar-btn").style.display = "block";
        } else {
          document.getElementById(key + "-btn").style.display = "none";
          document.getElementById(key + "-calendar-btn").style.display = "none";
        }
      });

      document.getElementById("loading").style.display = "none";
      document.getElementById("main").style.display = "block";
    }).catch(function() {
      // If the AJAX fails, still show the page
      document.getElementById("loading").style.display = "none";
      document.getElementById("main").style.display = "block";
    });
  </script>

